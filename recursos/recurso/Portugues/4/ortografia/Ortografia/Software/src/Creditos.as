package src{	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.display.SimpleButton;	import flash.accessibility.AccessibilityProperties;	import flash.events.Event;	import src.*;	public class Creditos extends MovieClip	{		//public var numeroQuadro:Number = 4;		private var objetoPrincipal:Object;				public function Creditos():void		{			//garante que os objetos globais de Principal (como o vetor de caminha de imagem) estejam carregados, depois monta o swf.			addEventListener (Event.ADDED_TO_STAGE, iniciar);			}		function iniciar (evt:Event)		{			objetoPrincipal =  parent.parent.parent;						objetoPrincipal.iniciaPosicoes ();						objetoPrincipal.proxima.removeEventListener(MouseEvent.CLICK, objetoPrincipal.proximaFC);			objetoPrincipal.proxima.enabled = false;						objetoPrincipal.pulse.visible = false;		}	}}