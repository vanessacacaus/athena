package src{	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.display.SimpleButton;	import flash.accessibility.AccessibilityProperties;	import flash.events.Event;	import src.*;	public class Abertura extends MovieClip	{		public var numeroQuadro:Number = 0;		private var objetoPrincipal:Object = new Object();				public function Abertura():void		{			//trace ("hihi");			//garante que os objetos globais de Principal (como o vetor de caminha de imagem) estejam carregados, depois monta o swf.			addEventListener (Event.ADDED_TO_STAGE, iniciar);		}		function iniciar (evt:Event)		{			objetoPrincipal =  parent.parent.parent;			objetoPrincipal.iniciaPosicoes ();					}		function botFunction()		{			objetoPrincipal.aparece(1);			objetoPrincipal.ajuda.visible = true;			objetoPrincipal.creditos.visible = true;			objetoPrincipal.sair_btn.visible = true;			objetoPrincipal.proxima.visible = true;			objetoPrincipal.fundoMenu.visible = true;			objetoPrincipal.questoes.visible = true;			objetoPrincipal.separador.visible = true;			objetoPrincipal.introducao.visible = true;			//Object(parent.parent.parent).addChildAt(fundoLinha, 0);		}				}}